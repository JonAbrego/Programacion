                        <%= f.input :reset_password_token, input_html: { value: resource.reset_password_token}, as: :hidden %>
                      </div>
                      <div class="form-group">
                        <%= f.input :password, label: "Nueva contrase単a", required: true, autofocus: true, hint: ("#{@minimum_password_length} caracteres minimo" if @minimum_password_length), input_html: { class: 'form-control' } %>
                      </div>
                    </div>
                    <div>
                      <div class="form-group">
                        <%= f.input :password_confirmation, label: "Confirma tu nueva contrase単a", required: true, input_html: { class: 'form-control' } %>
                      </div>
                    </div>

                    <div class="form-actions">
                      <%= f.button :submit, "Cambiar contrase単a", class: 'btn btn-primary btn-block' %>
                    </div>
                  <% end %>

